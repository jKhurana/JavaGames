package com.zetcode;

import java.awt.EventQueue;
import javax.swing.JFrame;

public class Application extends JFrame{
	
	public Application()
	{
		initUI();
	}
	
	private void initUI()
	{
		add(new Board());
		setSize(250,200);
		
		setTitle("Applicaiton");
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setLocaitonRelativeTo(null);
	}
}
